{
  "name": "cafe-employee-manager-backend",
  "version": "1.0.0",
  "description": "Backend API for Caf√© Employee Manager",
  "main": "dist/app.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm -- migration:generate -d src/infrastructure/database/config/typeorm.config.ts",
    "migration:run": "npm run typeorm -- migration:run -d src/infrastructure/database/config/typeorm.config.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/infrastructure/database/config/typeorm.config.ts",
    "seed": "node dist/infrastructure/database/seeds/seed.js"
  },
  "keywords": ["cafe", "employee", "management", "rest-api"],
  "author": "Sparsh Jain",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "typeorm": "^0.3.19",
    "reflect-metadata": "^0.2.1",
    "tsyringe": "^4.8.0",
    "class-validator": "^0.14.0",
    "class-transformer": "^0.5.1",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "swagger-ui-express": "^5.0.0",
    "swagger-jsdoc": "^6.2.8",
    "helmet": "^7.1.0",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.6",
    "@types/multer": "^1.4.11",
    "@types/uuid": "^9.0.7",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/compression": "^1.7.5",
    "@types/jest": "^29.5.11",
    "@types/supertest": "^6.0.2",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1"
  }
}
